{
  "name": "unocss-preset-inview",
  "type": "module",
  "version": "0.1.3",
  "description": "UnoCSS preset for scroll inview animation",
  "author": "subframe7536",
  "license": "MIT",
  "homepage": "https://github.com/subframe7536/unocss-preset-inview#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subframe7536/unocss-preset-inview.git"
  },
  "keywords": [
    "unocss",
    "unpreset",
    "unocss-preset",
    "IntersectionObserver"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./runtime": "./dist/runtime.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite playground",
    "build": "tsdown",
    "release": "bun run build && bumpp --all && npm publish",
    "format": "eslint . --fix",
    "test": "vitest",
    "test:update": "vitest --update",
    "deploy": "vite build playground"
  },
  "peerDependencies": {
    "@unocss/core": "*"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^1.3.2",
    "@types/node": "^22.17.2",
    "@unocss/eslint-plugin": "^66.4.2",
    "@vitest/eslint-plugin": "^1.3.4",
    "bumpp": "^10.2.3",
    "eslint": "^9.34.0",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2",
    "unocss": "^66.4.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
